Gem::Specification.new do |s|
  s.name     = 'jekyll-theme-ObJekyll'
  s.version  = '4.1.0'
  s.license  = 'MIT'
  s.summary  = 'Work with Obsidian.A content-first, sliding sidebar theme for Jekyll.'
  s.author   = 'yaleiyale'
  s.email    = 'deilookout@outlook.com'
  s.homepage = 'https://github.com/yaleiyale/jekyll-theme-ObJekyll'
  s.files    = `git ls-files -z`.split("\x0").grep(%r{^_(includes|layouts)/})
end
